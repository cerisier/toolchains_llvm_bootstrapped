diff -u a/libc/intrin/brain16.c b/libc/intrin/brain16.c
--- a/libc/intrin/brain16.c
+++ b/libc/intrin/brain16.c
@@ -1,5 +1,12 @@
-#if defined(__clang__) && !defined(__FLT32_MANT_DIG__)
-typedef float _Float32;   // if you're not compiling with -std=c23 yet
-typedef double _Float64;
-typedef long double __float80 __attribute__((__mode__(__XF__)));
-#endif
+#if defined(__clang__) && !defined(__FLT32_MANT_DIG__)
+typedef float _Float32;   // if you're not compiling with -std=c23 yet
+typedef double _Float64;
+#endif
